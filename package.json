{
    "name": "react-redux-boilerplate",
    "version": "1.0.0",
    "description": "Boilerplate for React+Redux webapps",
    "main": "webpack.config.dev.js",
    "dependencies": {
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "webpack": "^3.10.0"
    },
    "scripts": {
        "build": "webpack --config webpack.config.js --verbose",
        "dev": "yarn build -- --watch",
        "debug": "yarn build -- --env.debug=1",
        "prod": "yarn build -- --env.platform='PROD'"
    },
